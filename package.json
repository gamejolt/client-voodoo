{
	"name": "client-voodoo",
	"description": "This is an almighty package for anything that has to do with managing a library of games through the Game Jolt Client.",
	"version": "2.0.0-alpha.11",
	"bugs": {
		"url": "https://github.com/gamejolt/client-voodoo/issues"
	},
	"license": "MIT",
	"author": "Game Jolt Team",
	"repository": {
		"type": "git",
		"url": "https://github.com/gamejolt/client-voodoo.git"
	},
	"scripts": {
		"test": "mocha -u tdd --timeout 999999 --colors \"./build/**/patcher.test.js\"",
		"cover": "nyc mocha --compilers ts:ts-node/register --require source-map-support/register --full-trace -u tdd --timeout 999999 --colors src/**/*.test.ts",
		"build": "tsc",
		"watch": "tsc -w"
	},
	"nyc": {
		"include": [
			"src/**/*.ts"
		],
		"extension": [
			".ts"
		],
		"exclude": [
			"**/*.d.ts"
		],
		"require": [
			"ts-node/register"
		],
		"sourceMap": true,
		"instrument": true
	},
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"devDependencies": {
		"@types/bluebird": "^3.5.8",
		"@types/chai": "3.4.34",
		"@types/chai-as-promised": "0.0.30",
		"@types/mkdirp": "0.3.29",
		"@types/mocha": "2.2.41",
		"@types/node": "4.2.11",
		"@types/winreg": "1.2.30",
		"@types/xdg-basedir": "2.0.0",
		"chai": "3.4.1",
		"chai-as-promised": "6.0.0",
		"del": "2.2.2",
		"gulp": "3.9.1",
		"gulp-load-plugins": "1.5.0",
		"gulp-shell": "0.6.3",
		"mocha": "2.3.4",
		"nyc": "10.3.2",
		"prettier": "1.4.4",
		"run-sequence": "1.1.4",
		"source-map-support": "0.4.15",
		"ts-node": "3.0.4",
		"typescript": "2.4.2"
	},
	"optionalDependencies": {
		"applescript": "1.0.0",
		"ps-node": "0.1.6",
		"winreg": "1.2.4",
		"xdg-basedir": "2.0.0"
	},
	"dependencies": {
		"JSONStream": "github:gamejolt/JSONStream#no-shebang",
		"bluebird": "^3.5.0",
		"mkdirp": "0.5.1",
		"shell-escape": "0.2.0"
	}
}
